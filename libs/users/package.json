{
  "name": "@libs/users",
  "version": "0.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "imports": {
    "#src/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./addon-main.js": "./addon-main.cjs",
    "./*.css": "./dist/*.css",
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup --watch -c",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm run format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "prepack": "rollup -c",
    "start": "vite dev",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@embroider/addon-shim": "~1.10.2",
    "@triptyk/ember-input": "git+https://github.com/TRIPTYK/ember-common-ui/tree/upgrade-ember-ember-input-dist",
    "@triptyk/ember-input-validation": "git+https://github.com/TRIPTYK/ember-common-ui/tree/upgrade-ember-ember-input-validation-dist",
    "@triptyk/ember-ui": "git+https://github.com/TRIPTYK/ember-common-ui/tree/upgrade-ember-ember-ui-dist",
    "@warp-drive/core": "5.8.1",
    "@warp-drive/ember": "5.8.1",
    "@warp-drive/experiments": "~0.2.7",
    "@warp-drive/json-api": "5.8.1",
    "@warp-drive/legacy": "5.8.1",
    "@warp-drive/utilities": "5.8.1",
    "decorator-transforms": "~2.3.1",
    "ember-auto-import": "2.12.0",
    "ember-cli-page-object": "^2.3.2",
    "ember-immer-changeset": "git+https://github.com/TRIPTYK/ember-immer-changeset/tree/dist",
    "ember-intl": "~8.0.3",
    "ember-simple-auth": "^8.2.0",
    "ember-strict-application-resolver": "^0.1.1",
    "msw": "^2.12.7",
    "openapi-msw": "^2.0.0",
    "zod": "~4.3.6"
  },
  "devDependencies": {
    "@babel/core": "~7.28.6",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-transform-typescript": "^7.25.2",
    "@babel/runtime": "^7.25.6",
    "@ember/app-tsconfig": "~2.0.0",
    "@ember/library-tsconfig": "~2.0.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.2.1",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/addon-dev": "~8.2.0",
    "@embroider/compat": "~4.1.12",
    "@embroider/core": "~4.4.2",
    "@embroider/macros": "~1.19.6",
    "@embroider/vite": "~1.5.0",
    "@eslint/js": "^9.17.0",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "~1.1.0",
    "@glint/environment-ember-loose": "^1.5.2",
    "@glint/template": "~1.7.3",
    "@glint/tsserver-plugin": "^2.0.0",
    "@libs/utils": "workspace:^",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/browser-webdriverio": "^4.0.18",
    "@warp-drive/build-config": "5.8.1",
    "@warp-drive/core-types": "5.8.1",
    "babel-plugin-ember-template-compilation": "~4.0.0",
    "backend-app": "workspace:^",
    "concurrently": "^9.0.1",
    "ember-basic-dropdown": "^8.11.0",
    "ember-cli-flash": "^6.0.0",
    "ember-concurrency": "^5.1.0",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.3",
    "ember-resolver": "~13.1.1",
    "ember-resources": "^7.0.7",
    "ember-source": "~6.10.0",
    "ember-template-lint": "^7.9.0",
    "ember-truth-helpers": "^5.0.0",
    "ember-vitest": "^0.3.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "globals": "^17.1.0",
    "playwright": "^1.58.0",
    "prettier": "^3.4.2",
    "prettier-plugin-ember-template-tag": "^2.1.3",
    "rollup": "~4.57.0",
    "testem": "^3.15.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.19.1",
    "vite": "~7.3.1",
    "vitest": "^4.0.18",
    "webpack": "^5.104.1"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {
      "./components/forms/login-form.js": "./dist/_app_/components/forms/login-form.js",
      "./components/forms/user-form.js": "./dist/_app_/components/forms/user-form.js",
      "./handlers/auth.js": "./dist/_app_/handlers/auth.js",
      "./routes/dashboard/users/create.js": "./dist/_app_/routes/dashboard/users/create.js",
      "./routes/dashboard/users/edit.js": "./dist/_app_/routes/dashboard/users/edit.js",
      "./routes/dashboard/users/index.js": "./dist/_app_/routes/dashboard/users/index.js",
      "./routes/login.js": "./dist/_app_/routes/login.js",
      "./routes/logout.js": "./dist/_app_/routes/logout.js",
      "./schemas/users.js": "./dist/_app_/schemas/users.js",
      "./services/current-user.js": "./dist/_app_/services/current-user.js",
      "./services/user.js": "./dist/_app_/services/user.js",
      "./templates/dashboard/users/create.js": "./dist/_app_/templates/dashboard/users/create.js",
      "./templates/dashboard/users/edit.js": "./dist/_app_/templates/dashboard/users/edit.js",
      "./templates/dashboard/users/index.js": "./dist/_app_/templates/dashboard/users/index.js",
      "./templates/login.js": "./dist/_app_/templates/login.js"
    }
  }
}
