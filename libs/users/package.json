{
  "name": "@libs/users",
  "version": "0.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "imports": {
    "#src/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./addon-main.js": "./addon-main.cjs",
    "./*.css": "./dist/*.css",
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rolldown -c",
    "build:watch": "rolldown --watch -c",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm run format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "prepack": "rolldown -c",
    "start": "vite dev",
    "test": "vite build --mode=development --out-dir dist-tests && testem --file testem.cjs ci --port 0"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.9",
    "@embroider/util": "^1.13.5",
    "decorator-transforms": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-transform-typescript": "^7.25.2",
    "@babel/runtime": "^7.25.6",
    "@ember/app-tsconfig": "~2.0.0",
    "@ember/library-tsconfig": "~2.0.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.2.1",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/addon-dev": "~8.2.0",
    "@embroider/compat": "~4.1.12",
    "@embroider/core": "~4.4.2",
    "@embroider/macros": "~1.19.6",
    "@embroider/vite": "~1.5.0",
    "@eslint/js": "^9.17.0",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "~1.0.9",
    "@glint/template": "~1.7.3",
    "@glint/tsserver-plugin": "^2.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@triptyk/ember-input": "git+https://github.com/TRIPTYK/ember-common-ui/tree/upgrade-ember-ember-input-dist",
    "@triptyk/ember-input-validation": "git+https://github.com/TRIPTYK/ember-common-ui/tree/upgrade-ember-ember-input-validation-dist",
    "@triptyk/ember-ui": "git+https://github.com/TRIPTYK/ember-common-ui/tree/upgrade-ember-ember-ui-dist",
    "@types/qunit": "^2.19.12",
    "@warp-drive/build-config": "5.8.1",
    "@warp-drive/core": "5.8.1",
    "@warp-drive/core-types": "5.8.1",
    "@warp-drive/ember": "5.8.1",
    "@warp-drive/experiments": "~0.2.7",
    "@warp-drive/json-api": "5.8.1",
    "@warp-drive/legacy": "5.8.1",
    "@warp-drive/utilities": "5.8.1",
    "babel-plugin-ember-template-compilation": "~4.0.0",
    "backend-app": "workspace:^",
    "concurrently": "^9.0.1",
    "ember-concurrency": "^5.1.0",
    "ember-immer-changeset": "^1.0.2",
    "ember-intl": "~8.0.3",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.2",
    "ember-source": "~6.8.0",
    "ember-strict-application-resolver": "^0.1.0",
    "ember-template-lint": "^7.9.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "globals": "^16.1.0",
    "msw": "^2.12.7",
    "openapi-msw": "^2.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-ember-template-tag": "^2.0.4",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "rolldown": "~1.0.0-rc.1",
    "rollup": "^4.22.5",
    "testem": "^3.15.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.19.1",
    "vite": "^7.1.9",
    "webpack": "^5.104.1",
    "zod": "~4.3.6"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {
      "./components/dummy-form.js": "./dist/_app_/components/dummy-form.js",
      "./components/users-table.js": "./dist/_app_/components/users-table.js",
      "./handlers/users.js": "./dist/_app_/handlers/users.js",
      "./routes/users/create.js": "./dist/_app_/routes/users/create.js",
      "./routes/users/index.js": "./dist/_app_/routes/users/index.js",
      "./schemas/users.js": "./dist/_app_/schemas/users.js",
      "./templates/users/create.js": "./dist/_app_/templates/users/create.js",
      "./templates/users/index.js": "./dist/_app_/templates/users/index.js"
    }
  }
}
