{
  "name": "@libs/users-front",
  "version": "0.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "imports": {
    "#src/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./addon-main.js": "./addon-main.cjs",
    "./*.css": "./dist/*.css",
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "addon-main.cjs",
    "declarations",
    "translations",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup --watch -c",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm run format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "prepack": "rollup -c",
    "start": "vite dev",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@embroider/addon-shim": "catalog:",
    "@triptyk/ember-input": "catalog:",
    "@triptyk/ember-input-validation": "catalog:",
    "@triptyk/ember-ui": "catalog:",
    "@warp-drive/core": "catalog:",
    "@warp-drive/ember": "catalog:",
    "@warp-drive/experiments": "catalog:",
    "@warp-drive/json-api": "catalog:",
    "@warp-drive/legacy": "catalog:",
    "@warp-drive/utilities": "catalog:",
    "decorator-transforms": "catalog:",
    "ember-auto-import": "2.12.0",
    "ember-cli-page-object": "catalog:",
    "ember-immer-changeset": "catalog:",
    "ember-intl": "catalog:",
    "ember-simple-auth": "catalog:",
    "ember-strict-application-resolver": "catalog:",
    "msw": "catalog:",
    "openapi-msw": "catalog:",
    "zod": "~4.3.6"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-transform-typescript": "^7.25.2",
    "@babel/runtime": "^7.25.6",
    "@ember/app-tsconfig": "~2.0.0",
    "@ember/library-tsconfig": "catalog:",
    "@ember/string": "catalog:",
    "@ember/test-helpers": "^5.2.1",
    "@ember/test-waiters": "catalog:",
    "@embroider/addon-dev": "catalog:",
    "@embroider/compat": "catalog:",
    "@embroider/core": "catalog:",
    "@embroider/macros": "catalog:",
    "@embroider/vite": "catalog:",
    "@eslint/js": "^9.17.0",
    "@glimmer/component": "catalog:",
    "@glint/ember-tsc": "~1.1.0",
    "@glint/environment-ember-loose": "catalog:",
    "@glint/template": "catalog:",
    "@glint/tsserver-plugin": "catalog:",
    "@libs/repo-utils": "workspace:^",
    "@rollup/plugin-alias": "catalog:",
    "@rollup/plugin-babel": "^6.0.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "catalog:",
    "@warp-drive/build-config": "catalog:",
    "@warp-drive/core-types": "catalog:",
    "babel-plugin-ember-template-compilation": "~4.0.0",
    "@apps/backend": "workspace:^",
    "concurrently": "^9.0.1",
    "ember-basic-dropdown": "catalog:",
    "ember-cli-flash": "catalog:",
    "ember-concurrency": "catalog:",
    "ember-load-initializers": "catalog:",
    "ember-page-title": "catalog:",
    "ember-resolver": "catalog:",
    "ember-resources": "catalog:",
    "ember-source": "catalog:",
    "ember-template-lint": "^7.9.0",
    "ember-truth-helpers": "catalog:",
    "ember-vitest": "catalog:",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-n": "^17.15.1",
    "globals": "catalog:",
    "playwright": "^1.58.0",
    "prettier": "^3.4.2",
    "prettier-plugin-ember-template-tag": "^2.1.3",
    "rollup": "catalog:",
    "rollup-plugin-copy": "catalog:",
    "testem": "^3.15.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.19.1",
    "vite": "~7.3.1",
    "vitest": "^4.0.18",
    "webpack": "catalog:"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {
      "./components/forms/login-form.js": "./dist/_app_/components/forms/login-form.js",
      "./components/forms/user-form.js": "./dist/_app_/components/forms/user-form.js",
      "./handlers/auth.js": "./dist/_app_/handlers/auth.js",
      "./routes/dashboard/users/create.js": "./dist/_app_/routes/dashboard/users/create.js",
      "./routes/dashboard/users/edit.js": "./dist/_app_/routes/dashboard/users/edit.js",
      "./routes/dashboard/users/index.js": "./dist/_app_/routes/dashboard/users/index.js",
      "./routes/login.js": "./dist/_app_/routes/login.js",
      "./routes/logout.js": "./dist/_app_/routes/logout.js",
      "./schemas/users.js": "./dist/_app_/schemas/users.js",
      "./services/current-user.js": "./dist/_app_/services/current-user.js",
      "./services/user.js": "./dist/_app_/services/user.js",
      "./templates/dashboard/users/create.js": "./dist/_app_/templates/dashboard/users/create.js",
      "./templates/dashboard/users/edit.js": "./dist/_app_/templates/dashboard/users/edit.js",
      "./templates/dashboard/users/index.js": "./dist/_app_/templates/dashboard/users/index.js",
      "./templates/login.js": "./dist/_app_/templates/login.js"
    }
  }
}
